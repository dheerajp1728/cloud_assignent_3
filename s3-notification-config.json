{
  "LambdaFunctionConfigurations": [
    {
      "Id": "IndexPhotosOnUpload",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:381437599887:function:ai-photo-search-index-photos",
      "Events": ["s3:ObjectCreated:Put", "s3:ObjectCreated:Post"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "suffix",
              "Value": ".jpg"
            }
          ]
        }
      }
    },
    {
      "Id": "IndexPNGPhotos",
      "LambdaFunctionArn": "arn:aws:lambda:us-east-1:381437599887:function:ai-photo-search-index-photos",
      "Events": ["s3:ObjectCreated:Put", "s3:ObjectCreated:Post"],
      "Filter": {
        "Key": {
          "FilterRules": [
            {
              "Name": "suffix",
              "Value": ".png"
            }
          ]
        }
      }
    }
  ]
}
